<div id="<%= dom_id match %>">
  <p>
    <strong>Date:</strong>
    <%= match.date %>
  </p>

  <p>
    <strong>Score:</strong>
    <%= match.score %>
  </p>

  <p>
    <strong>Winner:</strong>
    <%= match.winner_id %>
  </p>

  <p>
    <strong>Loser:</strong>
    <%= match.loser_id %>
  </p>

  <p>
    <strong>League:</strong>
    <%= match.league_id %>
  </p>

</div>
